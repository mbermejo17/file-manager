{
    "name": "file-manager",
    "version": "1.0.0",
    "description": "Fielle Manager",
    "main": "server.js",
    "scripts": {
        "sass": "node-sass -o ./public/css/ ./scss/",
        "pug": "pug -w -P -p ./app/pug/includes/ ./src/pug/pages/ -o ./public/",
        "babel": "babel -w ./js/ -d ./public/js/",
        "browserify1": "browserify ./js/dashboard.js -o ./public/js/dashboard.js -t [ babelify --presets [ env ] ] -d ",
        "browserify2": "browserify ./js/logon.js -o ./public/js/logon.js -t [ babelify --presets [ env ] ] -d ",
        "watchify": "watchify ./js/dashboard.js -d -o ./public/js/dashboard.js -t [ babelify --presets [ env ] ] -v",
        "metalangs": "parallelshell \"npm run sass\" \"npm run pug\" \"npm run watchify\"",
        "devserve": "parallelshell \"npm run metalangs\" \"npm run serve\"",
        "start": "nodemon --watch app --watch public -e pug,scss,js,json server.js"
    },
    "author": "Manuel Bermejo <mbermejo17@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^8.6.4",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^8.0.0",
        "browser-sync": "^2.24.5",
        "browserify": "^16.2.2",
        "copy": "^0.3.2",
        "imagemin": "^5.3.1",
        "imagemin-cli": "^3.0.0",
        "imagemin-gifsicle": "^5.2.0",
        "imagemin-jpeg-recompress": "^5.1.0",
        "imagemin-pngquant": "^5.1.0",
        "imagemin-svgo": "^6.0.0",
        "imagemin-webp": "^4.1.0",
        "mkdirp": "^0.5.1",
        "node-sass": "^4.9.0",
        "node-sass-package-importer": "^5.1.3",
        "parallelshell": "^3.0.2",
        "postcss-cli": "^5.0.1",
        "pug": "^2.0.3",
        "pug-cli": "^1.0.0-alpha6",
        "rimraf": "^2.6.2",
        "type-of": "^2.0.1",
        "uglify-js": "^3.4.3",
        "uncss": "^0.16.2",
        "watchify": "^3.11.0"
    },
    "dependencies": {
        "cookie-parser": "^1.4.3",
        "express": "^4.16.3",
        "formidable": "^1.2.1",
        "fs-extra": "^6.0.1",
        "jsonwebtoken": "^8.3.0",
        "mime": "^2.3.1",
        "mime-type": "^3.0.5",
        "morgan": "^1.9.0",
        "normalize-path": "^3.0.0",
        "sqlite3": "^4.0.1"
    }
}
