{
    "name": "file-manager",
    "version": "1.0.0",
    "description": "Fielle Manager",
    "main": "server.js",
    "scripts": {
        "sass2": "node-sass scss/style.scss -o public/css && npx postcss public/css/style.css  --use autoprefixer -d public/css && npx postcss public/css/style.css --use postcss-csso -d public/css",
        "sass3": "node-sass scss/dashboard.scss -o public/css && npx postcss public/css/dashboard.css  --use autoprefixer -d public/css  && npx postcss public/css/dashboard.css --use postcss-csso -d public/css",
        "sass": "node-sass -o ./public/css/ ./scss/",
        "pug": "pug -w -P -p ./app/pug/includes/ ./src/pug/pages/ -o ./public/",
        "babel": "babel -w ./js/ -d ./public/js/",
        "browserify1": "browserify ./js/dashboard.js -o ./public/js/dashboard.js -t [ babelify --presets [ env ] ] -d ",
        "browserify2": "browserify ./js/logon.js -o ./public/js/logon.js -t [ babelify --presets [ env ] ] -d ",
        "browserify3": "browserify ./js/modules/fileManager.js -o ./public/js/modules/fileManager.js -t [ babelify --presets [ env ] ] -d ",
        "browserify4": "browserify ./js/modules/user.js -o ./public/js/modules/user.js -t [ babelify --presets [ env ] ] -d ",
        "browserify5": "browserify ./js/modules/admin.js -o ./public/js/modules/admin.js -t [ babelify --presets [ env ] ] -d ",
        "browserify6": "browserify ./js/modules/general.js -o ./public/js/modules/general.js -t [ babelify --presets [ env ] ] -d ",
        "watchify": "watchify ./js/dashboard.js -d -o ./public/js/dashboard.js -t [ babelify --presets [ env ] ] -v",
        "metalangs": "parallelshell \"npm run sass\" \"npm run pug\" \"npm run watchify\"",
        "devserve": "parallelshell \"npm run metalangs\" \"npm run serve\"",
        "start": "nodemon --watch app --watch public -e pug,scss,js,json server.js"
    },
    "author": "Manuel Bermejo <mbermejo17@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^8.6.5",
